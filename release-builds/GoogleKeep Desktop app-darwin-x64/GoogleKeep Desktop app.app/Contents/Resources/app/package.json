{
  "name": "GoogleKeep",
  "productName": "GoogleKeep Desktop app",
  "version": "1.0.1",
  "description": "A minimal desktop Google Keep Application.",
  "main": "main.js",
  "author": "Chandu",
  "license": "MIT",
  "scripts": {
    "start": "electron .",
    "pack": "build --dir",
    "dist": "build"
  },
  "dmg": {
    "contents": [
      {
        "x": 110,
        "y": 150
      },
      {
        "x": 240,
        "y": 150,
        "type": "link",
        "path": "/Applications"
      }
    ]
  },
  "linux": {
    "target": [
      "AppImage",
      "deb"
    ]
  },
  "win": {
    "target": "squirrel",
    "icon": "build/icon.ico"
  },
  "devDependencies": {
    "electron": "^4.0.4",
    "electron-builder": "^20.38.5",
    "electron-packager": "^13.1.0"
  },
  "build": {
    "files": [
      "config/*"
    ],
    "extraFiles": [
      "config"
    ]
  },
  "dependencies": {}
}
